<div class="border">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numero</th>
                <th>Descrição</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
            @if (ControleCaixaOutputView is not null)
            {
                @foreach (var controleCaixaOutputItem in ControleCaixaOutputView.Dados)
                {
                    <tr>
                        <td>@controleCaixaOutputItem.Id</td>
                        <td>@controleCaixaOutputItem.NumeroAtual</td>
                        <td>@controleCaixaOutputItem.TipoArquivo?.Descricao</td>
                        <td>
                            <a href="#">Editar</a> |
                            <a href="#">Excluir</a>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>

@code {
    [Parameter]
    public ControleCaixaOutputView? ControleCaixaOutputView { get; set; }
}